{
  "expo": {
    "name": "portal",
    "slug": "portal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fc-iphone.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#05668D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.mrahmanfade.portal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fc-android.png"
      },
      "package": "com.mrahmanfade.portal",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", "expo-document-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c5618959-5cdd-43f4-bd68-1251c4de0195"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c5618959-5cdd-43f4-bd68-1251c4de0195"
    }
  }
}
